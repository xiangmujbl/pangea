<adf-curation>
    <curation mainRegion="/ems/ems_f_z_enterprise_plants " viewRegion="/edm/plant_v1" chunkSize="1000" chunkBytes="40096"
              partitionPerServer="2" parallelism="2"
              executionClassName="com.jnj.pangea.view.processor.PantProcessor">
        <regions>

            <region path="/ems/ems_f_z_enterprise_plants ">
                <columns>
                    zPlantSourceSystem,zPlant,zEntPlantNumber,zSite,zEntPlantType,zRegion
                </columns>
                <relations>
                    <relation path="/edm/source_system_v1" isPrimaryKey="true">
                        <expression>localSourceSystem=/ems/ems_f_z_enterprise_plants.zPlantSourceSystem</expression>
                    </relation>

                    <relation path="/project_one/T001W" isPrimaryKey="false">
                        <expression>WERKS=/ems/ems_f_z_enterprise_plants.zPlant</expression>
                    </relation>

                </relations>
            </region>

            <region path="/edm/source_system_v1">
                <columns>
                    localSourceSystem,localSourceSystemName,sourceSystem,sourceSystemName
                </columns>
            </region>

            <region path="/project_one/T001W">
                <columns>
                    NAME1,LAND1,NODETYPE,WERKS
                </columns>
            </region>

            <region path="/ems/country_v1">
                <columns>
                    countryCode,countryName,localCountry
                </columns>
            </region>

            <region path="/edm/plant_v1">
                <columns>
                    sourceSystem,localPlant,localPlantName,plant,localPlanningRelevant,localCountry,country,site,plantName,localPlantType,plantType,region
                </columns>
            </region>

        </regions>
    </curation>
</adf-curation>

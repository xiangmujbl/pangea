<adf-curation>
    <curation mainRegion="/edm/plant_v1" viewRegion="/omp/gdm_location" chunkSize="1000" chunkBytes="40096"
              executionClassName="com.jnj.pangea.omp.gdm_location.controller.OMPGdmLocationEdmController"
              writeTo="all" topic="PANGEA_V1_omp_gdm_location_edm">
        <regions>

            <region path="/edm/plant_v1">
                <columns>
                    sourceSystem,localPlant,localPlanningRelevant,country,localCurrency,localPlantName,plantType,region,localCountry
                </columns>
                <relations>
                    <relation path="/plan/cns_plan_parameter" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/edm/country_v1" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_plant_attr" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/edm/currency_v1" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                </relations>
            </region>

            <region path="/plan/cns_plan_parameter">
                <columns>
                    parameterValue,sourceSystem,dataObject,attribute,parameter
                </columns>
            </region>

            <region path="/plan/cns_plant_attr">
                <columns>
                    localPlant,sourceSystem,planLocTypeId
                </columns>
            </region>

            <region path="/edm/country_v1">
                <columns>
                    sourceSystem,localCountry,consumerPlanningRegion
                </columns>
            </region>

            <region path="/edm/currency_v1">
                <columns>
                    sourceSystem,localCurrency,currencyCode
                </columns>
            </region>

            <region path="/omp/gdm_location">
                <columns>
                    locationId,
                    active,
                    activeFCTERP,
                    activeOPRERP,
                    activeSOPERP,
                    countryId,
                    currencyId,
                    customerId,
                    label,
                    locationTypeId,
                    regionId,
                    vendorId
                </columns>
            </region>

        </regions>
    </curation>
</adf-curation>
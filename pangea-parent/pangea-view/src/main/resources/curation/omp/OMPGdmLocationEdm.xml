<adf-curation>
    <curation mainRegion="/edm/plant_v1" viewRegion="/omp/gdm_location" chunkSize="1000" chunkBytes="40096"
              executionClassName="com.jnj.pangea.omp.gdm_location_edm.controller.OMPGdmLocationEdmController">
        <regions>

            <region path="/edm/plant_v1">
                <columns>
                    sourceSystem,localPlant,localPlanningRelevant,country,localCurrency,localPlantName,plantType,region
                </columns>
                <relations>
                    <relation path="/plan/cns_plan_parameter" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                </relations>
            </region>

            <region path="/plan/cns_plan_parameter">
                <columns>
                    parameterValue,sourceSystem,dataObject,attribute,parameter
                </columns>
            </region>

            <region path="/omp/gdm_location">
                <columns>
                    locationId,
                    active,
                    activeFCTERP,
                    activeOPRERP,
                    activeSOPERP,
                    countryId,
                    currencyId,
                    customerid,
                    label,
                    locationTypeId,
                    regionId,
                    vendorid
                </columns>
            </region>

        </regions>
    </curation>
</adf-curation>
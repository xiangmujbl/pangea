<adf-curation>
    <curation mainRegion="/edm/matl_bom" viewRegion="/omp/gdm_bom_element" chunkSize="1000" chunkBytes="40096"
              executionClassName="com.jnj.pangea.omp.gdm_bom_element.controller.OMPGdmBomElementController">
        <regions>

            <region path="/edm/matl_bom">
                <columns>
                    srcSysCd,matlNum,plntCd,altBomNum,sourceSystem,bomNum,bomUsgCd
                </columns>
                <relations>
                    <relation path="/plan/cns_plan_parameter" isPrimaryKey="false">
                        <expression>
                            sourceSystem=/edm/matl_bom.sourceSystem,
                            attribute=/edm/matl_bom.sourceSystem
                        </expression>
                    </relation>
                    <relation path="/edm/matl_prod_versn" isPrimaryKey="false">
                        <expression>
                            srcSysCd=/edm/matl_bom.srcSysCd,
                            matlNum=/edm/matl_bom.matlNum,
                            plntCd=/edm/matl_bom.plntCd,
                            altBomNum=/edm/matl_bom.altBomNum
                        </expression>
                    </relation>
                    <relation path="/edm/bom_item">
                        <expression>
                            srcSysCd=/edm/matl_bom.srcSysCd,
                            bomNum=/edm/matl_bom.bomNum
                        </expression>
                    </relation>
                    <relation path="/edm/bom_hdr">
                        <expression>
                            srcSysCd=/edm/matl_bom.srcSysCd,
                            bomNum=/edm/matl_bom.bomNum,
                            altBomNum=/edm/matl_bom.altBomNum
                        </expression>
                    </relation>
                </relations>
                <relations>
                    <relation path="/edm/material_plant_v1">
                        <expression></expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/material_plant_v1">
                <columns>
                    localGoodsReceiptProcessingTimeInDays, sourceSystem,localMaterialNumber,localPlant
                </columns>
            </region>
            <region path="/edm/bom_item">
                <columns>
                    bomNum,srcSysCd,bomCatCd, cmpntNum, bomItmNum, bomItmVldFromDt, dstrbtnKeyCd, fxQtyInd,
                    bomItmVldToDt, leadTimeOffst,cmpntQty,cmpntScrap_Pct, cmpntUomCd, bomItmVldFromDt
                </columns>
            </region>

            <region path="/edm/bom_hdr">
                <columns>
                    srcSysCd,altBomNum,bomNum,bomCatCd, bomVldFromDt, bomBaseQty, bomUomCd, bomVldFromDt
                </columns>
                <relations>
                    <relation path="/edm/bom_item">
                        <expression>
                            bomCatCd=/edm/bom_hdr.bomCatCd
                        </expression>
                    </relation>
                </relations>
            </region>

            <region path="/plan/cns_plan_parameter">
                <columns>
                    sourceSystem,dataObject,parameterValue,attribute
                </columns>
            </region>

            <region path="/edm/matl_prod_versn">
                <columns>
                    srcSysCd,plntCd,altBomNum,matlNum,rtngGrpCntrNum,rtngGrpCd,prdntVrsnNum,dstrbtnKeyCd,valToDt,valFromDt
                </columns>
                <relations>
                    <relation path="/edm/matl_mfg_rtng">
                        <expression>
                            srcSysCd=/edm/matl_prod_versn.srcSysCd,
                            matlNum=/edm/matl_prod_versn.matlNum,
                            plntCd=/edm/matl_prod_versn.plntCd,
                            rtngGrpCd=/edm/matl_prod_versn.rtngGrpCd,
                            rtngGrpcntrNum=/edm/matl_prod_versn.rtngGrpcntrNum
                        </expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/matl_mfg_rtng">
                <columns>
                    srcSysCd,rtngTypCd,rtngGrpcntrNum,plntCd,rntgGrpCntrNbr,matlNum,rtngGrpCd
                </columns>
                <relations>
                    <relation path="/edm/mfg_rtng_itm_nde">
                        <expression>
                            srcSysCd=/edm/matl_mfg_rtng.srcSysCd,
                            rtngTypCd=/edm/matl_mfg_rtng.rtngTypCd,
                            rtngGrpCd=/edm/matl_mfg_rtng.rtngGrpCd,
                            rntgGrpCntrNbr=/edm/matl_mfg_rtng.rntgGrpCntrNbr
                        </expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/mfg_rtng_itm_nde">
                <columns>
                    srcSysCd,rtngTypCd,rtngNdeNum,rtngGrpCntrNbr,rtngGrpCd
                </columns>
                <relations>
                    <relation path="/edm/mfg_rtng_itm">
                        <expression>
                            srcSysCd=/edm/mfg_rtng_itm_nde.srcSysCd,
                            rtngTypCd=/edm/mfg_rtng_itm_nde.rtngTypCd,
                            rtngGrpCd=/edm/mfg_rtng_itm_nde.rtngGrpCd,
                            rtngItmNum=/edm/mfg_rtng_itm_nde.rtngNdeNum,
                        </expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/mfg_rtng_itm">
                <columns>
                    rtngTypCd,rtngItmNum,srcSysCd,rtngGrpCd,operNum
                </columns>
            </region>

            <region path="/omp/gdm_bom_element">
                <columns>
                    bomElementId,active,activeFCTERP,activeOPRERP,activeSOPERP,batchId,bomId,bomType,bomUsage,comment,endEff,erpFeedbackQuantity,locationId,offset,offsetCalendarId,offsetPercentage,offsetType,planLevelId,productId,quantity,startEff
                </columns>
            </region>
        </regions>
    </curation>
</adf-curation>
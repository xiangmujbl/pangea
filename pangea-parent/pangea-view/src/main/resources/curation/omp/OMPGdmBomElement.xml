<adf-curation>
    <curation mainRegion="/edm/matl_bom" viewRegion="/omp/gdm_bom_element" chunkSize="1000" chunkBytes="40096"
              executionClassName="com.jnj.pangea.omp.gdm_bom_element.controller.OMPGdmBomElementController"
              writeTo="all" topic="PANGEA_V1_omp_gdm_bom_element">
        <regions>

            <region path="/edm/matl_bom">
                <columns>
                    srcSysCd,matlNum,plntCd,altBomNum,sourceSystem,bomNum,bomUsgCd,attribute
                </columns>
                <relations>
                    <relation path="/plan/cns_plan_parameter" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_plant_attr" isPrimaryKey="false">
                        <expression>
                            sourceSystem =/edm/matl_bom.srcSysCd
                        </expression>
                    </relation>
                    <relation path="/edm/matl_prod_versn" isPrimaryKey="false">
                        <expression>
                            srcSysCd=/edm/matl_bom.srcSysCd,matlNum=/edm/matl_bom.matlNum,plntCd=/edm/matl_bom.plntCd,altBomNum=/edm/matl_bom.altBomNum
                        </expression>
                    </relation>
                    <relation path="/edm/bom_item">
                        <expression>
                            srcSysCd=/edm/matl_bom.srcSysCd,bomNum=/edm/matl_bom.bomNum
                        </expression>
                    </relation>
                    <relation path="/edm/bom_hdr">
                        <expression>
                            srcSysCd=/edm/matl_bom.srcSysCd,bomNum=/edm/matl_bom.bomNum,altBomNum=/edm/matl_bom.altBomNum
                        </expression>
                    </relation>
                    <relation path="/edm/material_plant_v1">
                        <expression>
                            sourceSystem=/edm/matl_bom.sourceSystem,localMaterialNumber=/edm/matl_bom.matlNum,localPlant=/edm/matl_bom.plntCd
                        </expression>
                    </relation>
                    <relation path="/edm/material_global_v1">
                        <expression>
                            sourceSystem=/edm/matl_bom.srcSysCd
                        </expression>
                    </relation>
                    <relation path="/plan/cns_material_plan_status">
                        <expression>
                            sourceSystem=/edm/matl_bom.srcSysCd,
                            localPlant=/edm/matl_bom.plntCd
                        </expression>
                    </relation>
                </relations>
                <filter>
                    bomUsgCd:1 or bomUsgCd:2
                </filter>
            </region>


            <region path="/edm/material_plant_v1">
                <columns>
                    localGoodsReceiptProcessingTimeInDays, sourceSystem,localMaterialNumber,localPlant
                </columns>
            </region>
            <region path="/edm/bom_item">
                <columns>
                    bomNum,srcSysCd,bomCatCd,cmpntNum,bomItmNum,bomItmVldFromDt,dstrbtnKeyCd,fxQtyInd,bomItmVldToDt,leadTimeOffst,cmpntQty,cmpntScrap_Pct,cmpntUomCd
                </columns>
                <relations>
                    <relation path="/edm/material_global_v1">
                        localMaterialNumber=/edm/bom_item.cmpntNum
                    </relation>
                    <relation path="/plan/cns_material_plan_status">
                        localMaterialNumber=/edm/bom_item.cmpntNum
                    </relation>
                </relations>
            </region>

            <region path="/edm/bom_hdr">
                <columns>
                    srcSysCd,altBomNum,bomNum,bomCatCd,bomVldFromDt,bomBaseQty,bomUomCd
                </columns>
            </region>

            <region path="/plan/cns_plan_parameter">
                <columns>
                    sourceSystem,dataObject,parameterValue,attribute
                </columns>
                <filter>dataObject:SEND_TO_OMP</filter>
            </region>

            <region path="/plan/cns_plant_attr">
                <columns>
                    sourceSystem,localPlanningRelevant,planLocTypeId
                </columns>
                <filter>
                    localPlanningRelevant:X AND planLocTypeId:IM
                </filter>
            </region>
            <region path="/edm/matl_prod_versn">
                <columns>
                    srcSysCd,plntCd,altBomNum,matlNum,rtngGrpCntrNum,rtngGrpCd,prdntVrsnNum,dstrbtnKeyCd,valToDt,valFromDt
                </columns>
                <relations>
                    <relation path="/edm/matl_mfg_rtng">
                        <expression>
                            srcSysCd=/edm/matl_prod_versn.srcSysCd,matlNum=/edm/matl_prod_versn.matlNum,plntCd=/edm/matl_prod_versn.plntCd,rntgGrpCd=/edm/matl_prod_versn.rtngGrpCd,rntgGrpCntrNbr=/edm/matl_prod_versn.rtngGrpcntrNum
                        </expression>
                    </relation>
                </relations>
            </region>
            <region path="/edm/matl_mfg_rtng">
                <columns>
                    srcSysCd,rtngTypCd,rtngGrpcntrNum,plntCd,rntgGrpCntrNbr,matlNum,rntgGrpCd
                </columns>
                <relations>
                    <relation path="/edm/mfg_rtng_itm_nde">
                        <expression>
                            srcSysCd=/edm/matl_mfg_rtng.srcSysCd,rtngTypCd=/edm/matl_mfg_rtng.rtngTypCd,rtngGrpCd=/edm/matl_mfg_rtng.rntgGrpCd,rtngGrpCntrNbr=/edm/matl_mfg_rtng.rntgGrpCntrNbr
                        </expression>
                    </relation>
                </relations>
            </region>
            <region path="/edm/mfg_rtng_itm_nde">
                <columns>
                    srcSysCd,rtngTypCd,rtngNdeNum,rtngGrpCntrNbr,rtngGrpCd
                </columns>
                <relations>
                    <relation path="/edm/mfg_rtng_itm">
                        <expression>
                            srcSysCd=/edm/mfg_rtng_itm_nde.srcSysCd,rtngTypCd=/edm/mfg_rtng_itm_nde.rtngTypCd,rtngGrpCd=/edm/mfg_rtng_itm_nde.rtngGrpCd,rtngItmNum=/edm/mfg_rtng_itm_nde.rtngNdeNum
                        </expression>
                    </relation>
                </relations>
            </region>
            <region path="/edm/mfg_rtng_itm">
                <columns>
                    rtngTypCd,rtngItmNum,srcSysCd,rtngGrpCd,operNum,bsQty
                </columns>
            </region>
            <region path="/edm/material_global_v1">
                <columns>
                    primaryPlanningCode,localDpParentCode,sourceSystem,localMaterialNumber
                </columns>
            </region>
            <region path="/plan/cns_material_plan_status">
                <columns>
                    sourceSystem,localMaterialNumber,localPlant,spRelevant,noPlanRelevant,dpRelevant
                </columns>
            </region>
            <region path="/omp/gdm_bom_element">
                <columns>
                    bomElementId,active,activeFCTERP,activeOPRERP,activeSOPERP,batchId,bomId,bomType,bomUsage,comment,endEff,erpFeedbackQuantity,locationId,offset,offsetCalendarId,offsetPercentage,offsetPercType,planLevelId,productId,quantity,startEff
                </columns>
            </region>
        </regions>
    </curation>
</adf-curation>
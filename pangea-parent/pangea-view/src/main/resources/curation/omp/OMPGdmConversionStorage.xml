<adf-curation>
    <curation mainRegion="/plan/cns_dp_price" viewRegion="/omp/gdm_conversion_storage" chunkSize="1000"
              chunkBytes="40096"
              executionClassName="com.jnj.pangea.omp.gdm_conversion_storage.controller.OMPGdmConversionStorageController">
        <regions>

            <region path="/plan/cns_dp_price">
                <columns>
                    localMaterialNumber,country,currency,fromDate,toDate,salesPrice
                </columns>
                <relations>
                    <relation path="/edm/source_system_v1" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/edm/currency_v1" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/edm/country_v1" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_dp_price" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/edm/material_global_v1" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_cust_channel" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_dp_price" alias="/plan/cns_dp_price_clone">
                        <expression></expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/source_system_v1">
                <columns>
                    localSourceSystem,localSourceSystemName,sourceSystem,sourceSystemName
                </columns>
            </region>

            <region path="/edm/currency_v1">
                <columns>
                    localCurrency,currencyCode
                </columns>
            </region>

            <region path="/edm/country_v1">
                <columns>
                    localCountry,countryCode
                </columns>
            </region>

            <region path="/plan/cns_dp_price">
                <columns>
                    localMaterialNumber,country,salesPrice
                </columns>
            </region>

            <region path="/edm/material_global_v1">
                <columns>
                    localDpParentCode,
                </columns>
            </region>

            <region path="/plan/cns_cust_channel">
                <columns>
                    channel
                </columns>
            </region>

            <region path="/plan/cns_dp_price" alias="/plan/cns_dp_price_clone">
                <columns>
                    localMaterialNumber,salesPrice
                </columns>
            </region>

            <region path="/omp/gdm_conversion_storage">
                <columns>
                    sourceSystem,aggregationId,currencyId,dueDate,forecastUploadId,fromDueDate,salesPrice,unitId
                </columns>
            </region>

        </regions>
    </curation>
</adf-curation>
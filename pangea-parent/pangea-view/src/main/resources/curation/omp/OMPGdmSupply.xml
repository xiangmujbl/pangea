<adf-curation>
    <curation mainRegion="/edm/source_list_v1" viewRegion="/omp/gdm_supply" chunkSize="1000" chunkBytes="40096"
              executionClassName="com.jnj.pangea.omp.gdm_supply.controller.OMPGdmSupplyController"
              writeTo="all" topic="PANGEA_V1_omp_gdm_supply">
        <regions>

            <region path="/edm/source_list_v1">
                <columns>
                    sourceSystem,localPlant,localVendorAccountNumber,localMaterialNumber,localNumberofSourceListRecord,localSourceListRecordValidTo,localSourceListRecordValidFrom,localFixedvendor,localFixedOutlinePurchaseAgreementItem,localPurchasingOrganization,localAgreementNumber,localPlantfromWhichMaterialisProcured
                </columns>

                <relations>
                    <relation path="/edm/material_plant_v1" isPrimaryKey="false">
                        <expression>
                            localMaterialNumber=/edm/source_list_v1.localMaterialNumber
                        </expression>
                    </relation>
                    <relation path="/edm/plant_v1" isPrimaryKey="false">
                        <expression>
                            localPlant=/edm/source_list_v1.localPlant,
                            sourceSystem=/edm/source_list_v1.sourceSystem
                        </expression>
                    </relation>

                    <relation path="/plan/cns_material_plan_status" isPrimaryKey="false">
                        <expression>
                            localMaterialNumber=/edm/source_list_v1.localMaterialNumber,
                            localPlant=/edm/source_list_v1.localPlant,
                            sourceSystem=/edm/source_list_v1.sourceSystem
                        </expression>
                    </relation>
                    <relation path="/edm/material_global_v1" isPrimaryKey="false">
                        <expression>
                            localMaterialNumber=/edm/source_list_v1.localMaterialNumber
                        </expression>
                    </relation>

                    <relation path="/plan/cns_spl_pln_loc" isPrimaryKey="false">
                        <expression>
                            localNumber=/edm/source_list_v1.localVendorAccountNumber,
                            sourceSystem=/edm/source_list_v1.sourceSystem
                        </expression>
                    </relation>

                    <relation path="/plan/cns_process_type" isPrimaryKey="false">
                        <expression></expression>
                    </relation>

                </relations>

            </region>

            <region path="/edm/material_global_v1">
                <columns>
                    localMaterialNumber,materialNumber,sourceSystem,primaryPlanningCode
                </columns>
            </region>

            <region path="/edm/plant_v1">
                <columns>
                    sourceSystem,localPlant,plant,localPlanningRelevant,localMaterialNumber
                </columns>
            </region>

            <region path="/plan/cns_material_plan_status">
                <columns>
                    sourceSystem,localMaterialNumber,localPlant,materialNumber,localParentCode,ppc,active,dpRelevant,spRelevant,parentActive,noPlanRelevant
                </columns>
            </region>

            <region path="/edm/material_plant_v1">
                <columns>
                    localMaterialNumber,localPlant,sourceSystem,localMaximumLotSize,localMinimumLotSize,purchsngGrpCd,localRoundingValueForPoq,localPurchasingGroup,localSpecialProcurementType
                </columns>
            </region>

            <region path="/plan/cns_process_type">
                <columns>
                    processTypeId,processTypeDesc
                </columns>
            </region>

            <region path="/plan/cns_spl_pln_loc">
                <columns>
                    localNumber,vendorOrCustomer,sourceSystem,localPlant,localName
                </columns>
            </region>

            <region path="/omp/gdm_supply">
                <columns>
                    SupplyId,Active,ActiveOPRERP,ActiveSOPERP,FromDate,LABEL,LocationId,MaxQuantity,MinQuantity,Preference,PROCESSTYPEID,ProductId,PURCHASINGGROUP,PURCHASINGORGANIZATION,SupplierId,ToDate,TransportType,VENDORID,INCQuantity
                </columns>
            </region>
        </regions>
    </curation>
</adf-curation>
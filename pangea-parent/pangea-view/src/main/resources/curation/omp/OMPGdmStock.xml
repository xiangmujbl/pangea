<adf-curation>
    <curation mainRegion="/edm/process_order_v1" viewRegion="/omp/gdm_stock" chunkSize="1000" chunkBytes="40096"
              partitionPerServer="2" parallelism="2"
              executionClassName="com.jnj.pangea.omp.gdm_stock.GdmStock"
    >
        <regions>

            <region path="/edm/process_order_v1"
            >
                <columns>
                    matlNum,plntCd,mfgOrdrNum,sourceSystem,mfgOrdrItemNum,ordrType,ordrStts,ordrQty,rcvdQty,fctrNmrtrMeas,fctrDnmntrMeas,planClseDt,goodRcptLdDaysQty,prdtnSchdEndDt,prdntVrsnNum,delInd
                </columns>
                <relations>
                    <relation path="/edm/material_global_v1" isPrimaryKey="false"
                    >
                        <expression>
                            localMaterialNumber=/edm/process_order_v1.matlNum,localPlant=/edm/process_order_v1.plntCd
                        </expression>
                    </relation>
                    <relation path="/plan/cns_plan_object_filter" isPrimaryKey="false"
                    >
                        <expression></expression>
                    </relation>
                    <relation path="/edm/plant_v1" isPrimaryKey="false"
                    >
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_material_plan_status" isPrimaryKey="false"
                    >
                        <expression></expression>
                    </relation>
                </relations>

            </region>

            <region path="/edm/material_global_v1">
                <columns>
                    localMaterialNumber,localPlant,sourceSystem,primaryPlanningCode,materialNumber
                </columns>
            </region>
            <region path="/plan/cns_plan_object_filter">
                <columns>
                    sourceObjectTechName,sourceSystem,sourceObjectAttribute1,sourceObjectAttribute1Value,sourceObjectAttribute2,sourceObjectAttribute2Value
                </columns>
            </region>
            <region path="/edm/plant_v1">
                <columns>
                    sourceSystem,localPlant,localPlanningRelevant
                </columns>
            </region>
            <region path="/plan/cns_material_plan_status">
                <columns>
                    localMaterialNumber,localPlant,sourceSystem,noPlanRelevant
                </columns>
            </region>
            <region path="/omp/gdm_stock">
                <columns>
                    stockId,active,activeOPRERP,activeSOPERP,batchId,blockedQuantity,consignment,certaintyId,erpOrderId,inventoryLinkGroupId,vendorId,locationId,processId,processTypeId,productId,qualityQuantity,quantity,receiptDate,restrictedQuantity,returnsQuantity,startDate,stockType,transferQuantity,transitDate,unrestrictedQuantity
                </columns>
            </region>

        </regions>
    </curation>
</adf-curation>

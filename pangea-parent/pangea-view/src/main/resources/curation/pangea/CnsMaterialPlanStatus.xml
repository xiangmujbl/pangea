<adf-curation>
    <curation mainRegion="/edm/material_plant_v1" viewRegion="/pangea/cns_material_plan_status" chunkSize="1000"
              chunkBytes="40096"
              partitionPerServer="2" parallelism="2"
              executionClassName="com.jnj.pangea.pangea.cns_material_plan_status.controller.PangeaCnsMaterialPlanStatusController">
        <regions>

            <region path="/edm/material_plant_v1">
                <columns>
                    localMaterialNumber,localPlant,materialNumber,localDeletionFlagPlant,localMrpType,localMaterialType
                </columns>
                <relations>
                    <relation path="/edm/source_system_v1">
                        <expression></expression>
                    </relation>
                    <relation path="/edm/material_global_v1">
                        <expression>localMaterialNumber=/edm/material_plant_v1.localPlant,
                            localMaterialNumber=/edm/material_plant_v1.localMaterialNumber </expression>
                    </relation>
                    <relation path="/plan/cns_plan_parameter">
                        <expression></expression>
                    </relation>
                    <relation path="/plan/cns_material_incl">
                        <expression></expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/source_system_v1">
                <columns>
                    localSourceSystem,localSourceSystemName,sourceSystem,sourceSystemName
                </columns>
            </region>

            <region path="/edm/material_global_v1">
                <columns>
                    localMaterialType,localMaterialNumber,localDpParentCode,primaryPlanningCode
                </columns>
            </region>

            <region path="/plan/cns_plan_parameter">
                <columns>
                    sourceSystem,dataObject,attribute,parameter,inclExcl,parameterValue
                </columns>
            </region>

            <region path="/plan/cns_material_incl">
                <columns>
                    sourceSystem,localMaterialNumber,planningType,localPlant
                </columns>
            </region>

            <region path="/pangea/cns_material_plan_status">
                <columns>
                    sourceSystem,localMaterialNumber,localPlant,materialNumber,localParentCode,ppc,active,dpRelevant,spRelevant,parentActive,noPlanRelevant
                </columns>
            </region>

        </regions>
    </curation>
</adf-curation>

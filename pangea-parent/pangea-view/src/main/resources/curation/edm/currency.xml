<adf-curation>
    <curation mainRegion="/ems/ems_f_z_currencies" viewRegion="/edm/currency_v1" chunkSize="1000" chunkBytes="4096"
              executionClassName="com.jnj.pangea.edm.currency.controller.EDMCurrencyController">
        <regions>

            <region path="/ems/ems_f_z_currencies">
                <columns>zSourceSystem,zCode,zEntCodeIso4217Alpha,zName,zIso4217Numeric</columns>
                <filter>(*:* -zSourceSystem:\[EMS\])</filter>
                <relations>
                    <relation path="/edm/source_system_v1" isPrimaryKey="false">
                        <expression>localSourceSystem=/ems/ems_f_z_currencies.zSourceSystem</expression>
                    </relation>
                    <relation path="/ems/ems_f_z_currencies_clone" isPrimaryKey="false">
                        <expression></expression>
                    </relation>
                </relations>
            </region>

            <region path="/edm/source_system_v1">
                <columns>localSourceSystem,localSourceSystemName,sourceSystem,sourceSystemName</columns>
            </region>

            <region path="/ems/ems_f_z_currencies" alias="/ems/ems_f_z_currencies_clone">
                <columns>zSourceSystem,zCode,zEntCodeIso4217Alpha,zName,zIso4217Numeric</columns>

            </region>


            <region path="/edm/currency_v1">
                <columns>sourceSystem,localCurrency,currencyCode,currencyName,isoNumeric</columns>
            </region>

        </regions>
    </curation>
</adf-curation>
